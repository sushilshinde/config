
#Tomcat 
echo -e \\nInstalling Tomcat 9......................\\n

sudo wget -P ./Downloads http://www-us.apache.org/dist/tomcat/tomcat-9/v9.0.0.M21/bin/apache-tomcat-9.0.0.M21.zip

sudo mkdir -p ./Installations/Servers

sudo unzip -o -q ./Downloads/apache-tomcat-9.0.0.M21.zip -d ./Installations/Servers

sudo rm -rf ./Downloads/apache-tomcat-9.0.0.M21.zip
